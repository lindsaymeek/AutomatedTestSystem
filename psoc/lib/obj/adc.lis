 0001 4F           mov  X,SP       ;X will point at next pushed value
 0002 2103         and  A,03h
 0004 08           push A                  ;X points at copy of A
 0005 5D8B         mov  A,reg[ADC_AtoDcr3]
 0007 21FC         and  A,~03h       ;clear power bits
 0009 2B00         or   A,[ X ]
 000B 608B         mov  reg[ADC_AtoDcr3],A
 000D 18           pop  A
 000E 20           pop  X
 000F 7F           ret
