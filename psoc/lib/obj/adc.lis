 0014           ;;  Get_Samples:
 0014           ;;  SetPower:
 0014           ;;  Starts the A/D convertor and will place data is memory.  A flag
 0014           ;;  is set whenever a new data value is available.
 0014           ;;  INPUTS:  A passes the number of samples (0 is continous).
 0014           ;;  OUTPUTS: None.  
 0014           ;;------------------------------------------------------------------
 0014            ADC_GetSamples:
 0014           _ADC_GetSamples:
 0014 5300         mov [ADC_bIncrC],A         ;number of samples
 0016 43E103       or  reg[INT_MSK1],(ADC_TimerMask | ADC_CounterMask )
 0019                                                     ;Enable both interrupts
 0019 550001       mov [ADC_cTimerU],1       ;Force the Timer to do one cycle of rest
                IF ADC_NoAZ
 001C 438A20       or  reg[ADC_AtoDcr2],20h  ;force the Integrator into reset
                ENDIF
 001F 438B10       or  reg[ADC_AtoDcr3],10h
 0022 5500E0       mov [ADC_cCounterU],(-(1<<(ADC_NUMBITS - 7)));Initialize Counter
 0025 6221FF       mov reg[ADC_TimerDR1],ffh
 0028 6225FF       mov reg[ADC_CounterDR1],ffh
 002B 622301       mov reg[ADC_TimerCR0],01h ;enable the Timer
 002E 550000       mov [ADC_fIncr],00h       ;A/D Data Ready Flag is reset
 0031 7F           ret
