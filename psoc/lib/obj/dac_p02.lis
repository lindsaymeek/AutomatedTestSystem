                area text (ROM, REL)
                
                ;;---------------------------------------------------------------------------
                ;; Start / SetPower - Applies power setting to the module's SoCblocs
                ;;
                ;; INPUTS: A contains the power setting 0=Off, 1=Low, 2=Med, 3=High
                ;; OUTUTS: None
                ;;---------------------------------------------------------------------------
 0000            DAC_P02_Start:
 0000           _DAC_P02_Start:
 0000            DAC_P02_SetPower:
 0000           _DAC_P02_SetPower:
 0000 2103              and A, bPWRMASK
 0002 2930              or  A, bCR3             ; Set all other bits in addition to power
 0004 609F              mov reg[DAC_P02_CR3], A
 0006 7F                ret
 0007           
 0007           ;;---------------------------------------------------------------------------
 0007           ;; WriteBlind
 0007           ;; ----------
 0007           ;;
 0007           ;; Modify the DAC's update value without worrying about the clocks
 0007           ;;   Lowest overhead, but output may not settle to correct value until the
 0007           ;;   phi2 of next full cycle following the write.
 0007           ;;
 0007           ;; INPUTS: The accumulator, A, contains the input in the appropriate format.
 0007           ;;   The data format is determined by the setting of the DataFormat parameter
 0007           ;;   in the Device Editor.
 0007           ;;
 0007           ;; OUTPUTS: Analog output voltage reflects new value
 0007           ;;---------------------------------------------------------------------------
 0007            DAC_P02_WriteBlind:
 0007           _DAC_P02_WriteBlind:
 0007           
                  IF DAC_P02_OFFSETBINARY
                    ;; Data is an unsigned byte value in [0..62] (i.e., 63 unique values).
                    ;; Following converts it to 2's complement:
 0007 111F          sub  A, cOFFSET         ; Apply the offset
                  ENDIF
                  IF DAC_P02_OFFSETBINARY | DAC_P02_TWOSCOMPLEMENT
                    ;; Data is a byte in standard 2's complement form with value in [-31..+31]
                    ;; Following converts it to Sign & Magnitude form "00smmmmm"
                    ;;   where sign, "s", is 1 for negative numbers; 0 for positive
                    ;;   and "m" is the magnitude.
 0009 64            asl  A                  ; Multiply by 2 and put sign in Carry flag
 000A D007          jnc  BlindPositive
 000C               ;; Neg to pos by "Invert & Add 1" procedure, but data is shifted!
 000C 73            cpl  A                  ; bit 0 is a "1" so, following 1 byte "inc" works
 000D 74            inc  A                  ;   (otherwise, we'd have to "add A, 2")
 000E 2940          or   A, 40h             ; Make it negative by forcing sign bit
 0010 8006          jmp  BlindMagSet
 0012           BlindPositive:
 0012 40            nop
 0013 40            nop
 0014 40            nop
 0015 8001          jmp  BlindMagSet
 0017           BlindMagSet:
 0017 67            asr  A                  ; Divide by two to finish up
                  ENDIF
                
                    ;; Data is in Sign & Magnitude form.
                    ;; Set FCap and ClockPhase bits
 0018 2980          or   A, DAC_P02_CR0_HIBITS
 001A 609C          mov  reg[DAC_P02_CR0], A
 001C 7F            ret
 001D           
 001D           ;;---------------------------------------------------------------------------
 001D           ;; WriteStall
 001D           ;; ----------
 001D           ;;
 001D           ;; Modify the DAC's update value, stalling the CPU if necessary.
 001D           ;;   This routine should be used with fast analog clocks or when the
 001D           ;;   resulting interrupt latencies, comparable to the update period,
 001D           ;;   can be tolerated comfortably.
 001D           ;;
 001D           ;; INPUTS: The accumulator, A, contains the input in the appropriate format.
 001D           ;;   The data format is determined by the setting of the DataFormat parameter
 001D           ;;   in the Device Editor.
 001D           ;;
 001D           ;; OUTPUTS: Analog output voltage reflects new value
 001D           ;;---------------------------------------------------------------------------
 001D            DAC_P02_WriteStall:
 001D           _DAC_P02_WriteStall:
 001D           
                  IF DAC_P02_OFFSETBINARY
                    ;; Data is an unsigned byte value in [0..62] (i.e., 63 unique values).
                    ;; Following converts it to 2's complement:
 001D 111F          sub  A, cOFFSET         ; Apply the offset
                  ENDIF
                  IF DAC_P02_OFFSETBINARY | DAC_P02_TWOSCOMPLEMENT
                    ;; Data is a byte in standard 2's complement form with value in [-31..+31]
                    ;; Following converts it to Sign & Magnitude form "00smmmmm"
                    ;;   where sign, "s", is 1 for negative numbers; 0 for positive
                    ;;   and "m" is the magnitude.
 001F 64            asl  A                  ; Multiply by 2 and put sign in Carry flag
 0020 D007          jnc  StallPositive
 0022 73            cpl  A                  ; "Invert" step of complement 2's complement
 0023 74            inc  A                  ; "Add 1"  step of complement 2's complement
 0024 2940          or   A, 40h             ; Make it negative
 0026 8006          jmp  StallMagSet
 0028           StallPositive:
 0028 40            nop
 0029 40            nop
 002A 40            nop
 002B 8001          jmp  StallMagSet
 002D           StallMagSet:
 002D 67            asr  A                  ; Divide by two to finish conversion
                  ENDIF
                
                    ;; Data is in Sign & Magnitude form.
                    ;; Set FCap and ClockPhase bits
 002E 2980          or   A, DAC_P02_CR0_HIBITS
 0030 436501        or    reg[ASY_CR], ASY_CR_SYNCEN
 0033 609C          mov  reg[DAC_P02_CR0], A
 0035 4165FE        and   reg[ASY_CR], ~ASY_CR_SYNCEN
 0038 7F            ret
 0039           
 0039           ;;---------------------------------------------------------------------------
 0039           ;; Stop - Cuts power to the user module.
 0039           ;;
 0039           ;; INPUTS:  None
 0039           ;; OUTPUTS: None
 0039           ;;---------------------------------------------------------------------------
 0039            DAC_P02_Stop:
 0039           _DAC_P02_Stop:
 0039 419FFC        and reg[DAC_P02_CR3], ~bPWRMASK
 003C 7F            ret
